{% extends 'base.html.twig' %}

{% block title %}SubCategory{% endblock %}

{% block body %}
<div class="container">
    <h1>SubCategory</h1><br>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ sub_category.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ sub_category.name }}</td>
            </tr>
        </tbody>
    </table>

    <a class="btn btn-outline-dark" href="{{ path('app_sub_category_index') }}">back to list</a>

    <a class="btn btn-outline-primary" href="{{ path('app_sub_category_edit', {'id': sub_category.id}) }}">edit</a>

</div>
{% endblock %}
